		"history":"2w",
		"trends":"0d",
		"triggers": [
			{
				"prototype":"psu.crit"
			},
			{
				"prototype":"psu.warn"
			}
		],
        "translations":{
          "RU":{
            "name":"Статус блока питания"
          }
        }
	},
	{
		"id":"sensor.fan.status",
		"name":"Fan status",
		"group":"Fans",
		"delay":"3m",
		"history":"2w",
		"trends":"0d",
		"triggers": [
			{
				"prototype":"fan.crit"
			},
			{
				"prototype":"fan.warn"
			}
		],
        "translations":{
          "RU":{
            "name":"Статус вентилятора"
          }
        }
	},
	{
		"id":"sensor.fan.speed.percentage",
		"name":"Fan speed, %",
		"group":"Fans",
		"units":"%",
		"history": "30d",
        "translations":{
          "RU":{
            "name":"Скорость вращения вентилятора, %"
          }
        }
	},
	{
		"id":"sensor.fan.speed",
		"name":"Fan speed",
		"group":"Fans",
		"units":"rpm",
		"history": "30d",
        "translations":{
          "RU":{
            "name":"Скорость вращения вентилятора"
          }
        }
	}
]